{
  "extends": [
    "airbnb-typescript/base",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "jest": true,
    "node": true,
    "browser": false
  },
  "plugins": ["prettier"],
  "rules": {
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "import/no-unresolved": "off",
    "no-console": "off",
    "prettier/prettier": "error"
  }
}
